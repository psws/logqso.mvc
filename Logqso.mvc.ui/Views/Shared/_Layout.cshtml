<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - App.</title>
    @*@Styles.Render("~/Content")*@
    @{
        var theme = Themes.CurrentTheme;
    }

    @{
        if (true)
        {
            @Styles.Render(Logqso.mvc.ui.Helpers.JQueryUI.Bundle(theme))
        }
        else
        {
            // does not work with BundleTable.EnableOptimizations = true
            @Styles.Render("~/Cupertono")
        }
}
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqgrid")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/logqso")





</head>
<body id="body">
    <div class="navbar navbar-inverse @*navbar-fixed-top*@" style="margin-bottom: 0;">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("ContestQso", "logqso", "logqso", new { area = "" }, new { @class = "navbar-brand" })
                @*@Html.ActionLink("help info", "Help", "logqso", new { area = "" }, new { @class = "help-intro" })*@
                <a id="Vid_Intro" data-modal-id="puVidIntro" class=" help-intro">
                    help info
                </a>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Home", "Index", "Home")</li>*@
                    @*<li>@Html.ActionLink("About", "About", "Home")</li>*@
                    <li>@Html.ActionLink("About", "About", "logqso")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "logqso")</li>
                    @if (Logqso.mvc.ui.MvcApplication.IsDebug == true)
                    {
                        <li>@Html.ActionLink("API", "Index", "Help", new { area = "" }, null)</li>
                    }
                </ul>
                @if (Logqso.mvc.ui.MvcApplication.IsDebug == true)
                {
                    @Html.Partial("_LoginPartial")
                }
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        @*<hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>*@
    </div>

    @*@Scripts.Render("~/bundles/jquery")*@

    @RenderSection("scripts", required: false)
</body>
</html>
